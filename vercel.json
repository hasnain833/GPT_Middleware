{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/health", "dest": "/api/index.js" },
    { "src": "/excel/(.*)", "dest": "/api/index.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ]
}
